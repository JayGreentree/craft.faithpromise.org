{% set campuses = craft.entries({
    section: 'campuses',
    with: [
    'campusPhoto',
    'serviceTimes'
]
}) %}

{% embed '_layout/section.twig' with params %}
    {% block body %}
        {% include '_items/_cards.twig' with { 'items': campuses, 'compact': true } %}
    {% endblock %}
{% endembed %}